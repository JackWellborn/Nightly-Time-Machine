<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.jackwellborn.nightlytimemachine</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/bash</string>
		<string>%SCRIPT_PATH%Mount Time Machine Disk and Back Up.sh</string>
	</array>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Hour</key>
		<integer>%HOUR%</integer>
		<key>Minute</key>
		<integer>0</integer>
	</dict>
</dict>
</plist>
